<Window x:Class="EXModbus.EXModebusMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EXModbus"
        mc:Ignorable="d"
        Title="디밍제어 Ver1.0.2 (리산테크) 홍지문터널 문화촌전기실" 
        Icon="/Icon/tunnel_icon_38498_qWK_icon.ico"
        Width="1024" 
        MinWidth="1024"
        Height="701" 
        MinHeight="800"
        Loaded="Window_Loaded"
        ContentRendered="Window_ContentRendered"
        Closing="Window_Closing" 
        WindowStartupLocation="CenterScreen">
    <Window.CommandBindings>
        <CommandBinding Command="local:CustomCommands.Connect"
                        Executed="Connect_Executed"
                        CanExecute="Connect_CanExecute"/>
        <CommandBinding Command="local:CustomCommands.Disconnect"
                        Executed="Disconnect_Executed"
                        CanExecute="Disconnect_CanExecute"/>
        <CommandBinding Command="local:CustomCommands.ResetCnt"
                        Executed="ResetCnt_Executed"
                        CanExecute="ResetCnt_CanExecute"/>
        <CommandBinding Command="local:CustomCommands.ClickToggleBtn"
                        Executed="ClickToggleBtn_Executed"
                        CanExecute="ClickToggleBtn_CanExecute"/>
        <CommandBinding Command="local:CustomCommands.SendDataBtn"
                        Executed="SendDataBtn_Executed"
                        CanExecute="SendDataBtn_CanExecute"/>
    </Window.CommandBindings>
    <Grid UseLayoutRounding="True" Background="LightSteelBlue">
        <TabControl TabStripPlacement="Left" Margin="0,5,5,5" SelectedIndex="0">
            <TabItem Style="{StaticResource TabItemStyleRotate}" >
                <TabItem.Header>
                    <TextBlock Text="디밍제어기" Style="{StaticResource HeaderTextBlockStyle}"/>
                </TabItem.Header>
                <Grid Background="WhiteSmoke" Margin="0,0,0,0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1.0*" />
                        <ColumnDefinition Width="1.0*" />
                    </Grid.ColumnDefinitions>
                    <!--제어기 접속-->
                    <GroupBox Grid.Row="0" Grid.Column="0" Margin="5,5,5,2"
                  Style="{StaticResource GroupBoxStyle1}"
                  Foreground="White" x:Name="gbConnect">
                        <GroupBox.Header>
                            <Run FontWeight="Bold" FontSize="12">제어기 접속</Run>
                        </GroupBox.Header>
                        <StackPanel Orientation="Horizontal" Margin="165,-25,0,5">
                            <Label Content="IP" FontSize="16" VerticalAlignment="Center" Margin="0,0,5,0" />
                            <TextBox Text="{Binding Ip}" VerticalAlignment="Center"
                         Foreground="Navy" FontWeight="Bold"
                         FontSize="16" Background="AliceBlue"
                         BorderThickness="2" Width="140"/>
                            <Button Command="local:CustomCommands.Connect"
                        Style="{StaticResource BaseButtonStyle}" Margin="20,0,5,0" Width="auto">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="접속" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </Button>
                            <Button Command="local:CustomCommands.Disconnect"
                        Style="{StaticResource BaseButtonStyle}" Margin="10,0,5,0" Width="auto">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="끊기" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </Button>
                        </StackPanel>
                    </GroupBox>
                    <!--접속 상태-->
                    <GroupBox Grid.Row="0" Grid.Column="1" Margin="5,5,5,2"
                  Style="{StaticResource GroupBoxStyle1}"
                  Foreground="White"
                  x:Name="gbConnectionStat">
                        <GroupBox.Header>
                            <Run FontWeight="Bold" FontSize="12">접속 상태</Run>
                        </GroupBox.Header>
                        <StackPanel Orientation="Horizontal" Margin="165,-25,0,5">
                            <Label FontSize="16" FontWeight="Bold" VerticalAlignment="Center" Margin="0,0,0,0" 
                                   Foreground="{Binding ConnStatForgBrush}" 
                                   Background="{Binding ConnStatBackgBrush}" 
                                   Content="{Binding ConnStat}"/>
                            <!--<Label Content="송신" FontSize="16" VerticalAlignment="Center" Margin="0,0,0,0" />
                            <TextBox Text="{Binding TxFrmCnt}" VerticalAlignment="Center"
                         Foreground="Navy" FontWeight="Bold"
                         FontSize="16" Background="AliceBlue"
                         BorderThickness="2" Width="60" HorizontalContentAlignment="Right" />
                            <Label Content="수신" FontSize="16" VerticalAlignment="Center" Margin="10,0,0,0" />
                            <TextBox Text="{Binding RxFrmCnt}" VerticalAlignment="Center"
                         Foreground="Navy" FontWeight="Bold"
                         FontSize="16" Background="AliceBlue"
                         BorderThickness="2" Width="60" HorizontalContentAlignment="Right" />
                            <Button Style="{StaticResource BaseButtonStyle}" 
                        Command="local:CustomCommands.ResetCnt"
                        Margin="20,0,5,0" Width="auto">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="초기화" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </Button>-->
                        </StackPanel>
                    </GroupBox>
                    <!--주전기실 디밍제어기-->
                    <GroupBox Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Margin="5,2,5,2"
                  Style="{StaticResource GroupBoxStyle1}"
                  Foreground="White"
                  x:Name="gbMainController" Visibility="Hidden">
                        <GroupBox.Header>
                            <Run FontWeight="Bold" FontSize="12">주전기실 디밍제어기</Run>
                        </GroupBox.Header>
                        <StackPanel Orientation="Horizontal" Margin="160,-25, 0, 5">
                            <Label FontSize="16" VerticalAlignment="Center" Margin="0,0,5,0">교통량</Label>
                            <TextBox Text="{Binding TrafficDesign}" VerticalAlignment="Center"
                         Foreground="Navy" FontWeight="Bold" IsReadOnly="True"
                         FontSize="16" Background="AliceBlue"
                         BorderThickness="2" Width="100" HorizontalContentAlignment="Center" />
                            <TextBox Text="{Binding TrafficReal}" VerticalAlignment="Center"
                         Foreground="Navy" FontWeight="Bold" IsReadOnly="True" Margin="10,0,0,0"
                         FontSize="16" Background="AliceBlue"
                         BorderThickness="2" Width="100" HorizontalContentAlignment="Center" />
                            <Label FontSize="16" VerticalAlignment="Center" Margin="30,0,0,0">등기구 총수량</Label>
                            <TextBox Text="{Binding H708B}" VerticalAlignment="Bottom"
                         Foreground="Navy" FontWeight="Bold" IsReadOnly="True"
                         FontSize="16" Background="AliceBlue"
                         BorderThickness="2" Width="60" HorizontalContentAlignment="Center" Margin="0,0,0,4" />
                            <StackPanel>
                                <ToggleButton x:Name="tbCount" Width="auto" Margin="30,0,0,0" Style="{StaticResource PopupToggleButtonStyle}">
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Text="상세수량" Width="auto" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                    </StackPanel>
                                </ToggleButton>
                                <Popup IsOpen="{Binding IsChecked, ElementName=tbCount}" 
                           StaysOpen="False">
                                    <Border Background="Lavender" BorderThickness="2" BorderBrush="CadetBlue">
                                        <StackPanel Margin="5" >
                                            <Label FontSize="14" HorizontalContentAlignment="Center"
                                       Margin="5" 
                                       Background="#FF4169AA" Foreground="White"
                                       FontWeight="Bold" 
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Stretch" 
                                       Content="주전기실"/>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="터널등 수량(100W)" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H708C}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="터널등 수량(200W)" Width="160" FontSize="14" Margin="0" FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H708D}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="가로등 수량(100W)" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H708E}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="가로등 수량(150W)" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H708F}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="가로등 수량(250W)" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H7090}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="모뎀 불량 총수량" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H7091}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="LED모듈 불량 총수량" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H7092}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="SMPS 불량 총수량" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H7093}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="등기구 누전 총수량" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H7094}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="중계기 고장 총수량" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H7095}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="조명제어기 장애 총수량" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H7096}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="GPS 장애 총수량" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H7097}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="휘도센서 장애 총수량" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H7098}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Label Content="조도센서 장애 총수량" Width="160" FontSize="14" Margin="0"  FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                <TextBox Text="{Binding H7099}" VerticalAlignment="Center"
                                     Foreground="Navy" FontWeight="Bold"
                                     FontSize="14" Background="AliceBlue" 
                                     BorderThickness="2" Width="40" HorizontalContentAlignment="Right" />
                                            </StackPanel>
                                        </StackPanel>
                                    </Border>
                                </Popup>
                            </StackPanel>
                        </StackPanel>
                    </GroupBox>
                    <!--터널등 디밍제어기 상태-->
                    <GroupBox Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2"  
                  Margin="5,2,5,2" Foreground="White"
                  Style="{StaticResource GroupBoxStyle1}"
                  x:Name="gbTunnelDimmingControlStat">
                        <GroupBox.Header>
                            <Run FontWeight="Bold" FontSize="12">터널등 디밍 제어기 상태</Run>
                        </GroupBox.Header>
                        <StackPanel Orientation="Horizontal" Margin="160,-30,0,0" >
                            <TextBlock Background="{Binding H73A1B00Brush}" Foreground="Black" Margin="5" Padding="5" FontSize="16" Text="CPU이상"  />
                            <TextBlock Background="{Binding H73A1B01Brush}" Foreground="Black" Margin="5" Padding="5" FontSize="16" Text="POWER이상"  />
                            <TextBlock Background="{Binding H73A0B00Brush}" Foreground="Black" Margin="5" Padding="5" FontSize="16" Text="통신이상"  />
                            <TextBlock Background="{Binding H7080B01Brush}" Foreground="Black" Margin="5" Padding="5" FontSize="16" Text="화재 운전"  />
                            <TextBlock Background="{Binding H7080B07Brush}" Foreground="Black" Margin="5" Padding="5" FontSize="16" Text="Local수동"  />
                            <Button Command="local:CustomCommands.SendDataBtn" Margin="50,0,0,0"
                                    CommandParameter="H7084B00"
                                    Style="{StaticResource BaseButtonStyle}" Width="80">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="화재발생" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </Button>
                            <Button Command="local:CustomCommands.SendDataBtn" Margin="10,0,0,0"
                                    CommandParameter="H7084B01"
                                    Style="{StaticResource BaseButtonStyle}" Width="80">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="화재복구" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </Button>
                        </StackPanel>
                    </GroupBox>
                    <!--맑음/흐림 운전상태-->
                    <GroupBox Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="1"  
                  Margin="5,2,5,2" Foreground="White"
                  Style="{StaticResource GroupBoxStyle1}"
                  x:Name="gbClearCloudyCondition">
                        <GroupBox.Header>
                            <Run FontWeight="Bold" FontSize="12">수광부 운전상태</Run>
                        </GroupBox.Header>
                        <StackPanel Orientation="Horizontal" Margin="160,-30,0,0">
                            <ToggleButton Command="local:CustomCommands.ClickToggleBtn"
                                          CommandParameter="H7085B00"
                                          IsChecked="{Binding H7080B02}" Style="{StaticResource ToggleButtonStyle}">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="조도" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </ToggleButton>
                            <ToggleButton Command="local:CustomCommands.ClickToggleBtn"
                                          CommandParameter="H7085B01"
                                          IsChecked="{Binding H7080B03}" Style="{StaticResource ToggleButtonStyle}">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="휘도" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </ToggleButton>
                        </StackPanel>
                    </GroupBox>
                    <!--모드-->
                    <GroupBox Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="1"  
                  Margin="5,2,5,2" Foreground="White"
                  Style="{StaticResource GroupBoxStyle1}"
                  x:Name="gbMode">
                        <GroupBox.Header>
                            <Run FontWeight="Bold" FontSize="12">모드</Run>
                        </GroupBox.Header>
                        <StackPanel Orientation="Horizontal" Margin="165,-30,0,0">
                            <ToggleButton Command="local:CustomCommands.ClickToggleBtn"
                                          CommandParameter="H7083B00"
                                          IsChecked="{Binding H7080B00I}" Style="{StaticResource ToggleButtonStyle}" Width="80" >
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="Local" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </ToggleButton>
                            <ToggleButton Command="local:CustomCommands.ClickToggleBtn"
                                          CommandParameter="H7083B01"
                                          IsChecked="{Binding H7080B00}" Style="{StaticResource ToggleButtonStyle}" Width="80">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="Remote" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </ToggleButton>
                            <ToggleButton Command="local:CustomCommands.ClickToggleBtn"
                                          CommandParameter="H7085B04"
                                          IsChecked="{Binding H7080B06}" Style="{StaticResource ToggleButtonStyle}" Width="auto">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="Remote수동" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </ToggleButton>
                        </StackPanel>
                    </GroupBox>
                    <!--주간등 운전상태-->
                    <GroupBox Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="1"  
                  Margin="5,2,5,2" Foreground="White"
                  Style="{StaticResource GroupBoxStyle1}"
                  x:Name="gbDaylightOperatingState">
                        <GroupBox.Header>
                            <Run FontWeight="Bold" FontSize="12">주간등 운전상태</Run>
                        </GroupBox.Header>
                        <StackPanel Orientation="Horizontal" Margin="160,-30,0,0">
                            <ToggleButton Command="local:CustomCommands.ClickToggleBtn"
                                          CommandParameter="H7085B02"
                                          IsChecked="{Binding H7080B04}" Style="{StaticResource ToggleButtonStyle}">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="조도/휘도" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </ToggleButton>
                            <ToggleButton Command="local:CustomCommands.ClickToggleBtn"
                                          CommandParameter="H7085B03"
                                          IsChecked="{Binding H7080B05}" Style="{StaticResource ToggleButtonStyle}">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="시간" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </ToggleButton>
                        </StackPanel>
                    </GroupBox>
                    <!--전체설정-->
                    <GroupBox Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="1"  
                  Margin="5,2,5,2" Foreground="White"
                  Style="{StaticResource GroupBoxStyle1}">
                        <GroupBox.Header>
                            <Run FontWeight="Bold" FontSize="12">전체설정</Run>
                        </GroupBox.Header>
                        <StackPanel Orientation="Horizontal" Margin="165,-30,0,0">
                            <Button Command="local:CustomCommands.SendDataBtn"
                                    CommandParameter="H7089B00"
                                    Style="{StaticResource BaseButtonStyle}" Width="80">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="전체점등" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </Button>
                            <Button Command="local:CustomCommands.SendDataBtn"
                                    CommandParameter="H7089B01"
                                    Style="{StaticResource BaseButtonStyle}" Width="80" Visibility="Hidden">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="전체소등" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                </StackPanel>
                            </Button>
                        </StackPanel>

                    </GroupBox>
                    <!--상행선, 하행선 제어-->
                    <Grid Grid.Row="5" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1.0*" />
                            <RowDefinition Height="1.0*" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="4.0*" />
                            <ColumnDefinition Width="1.0*" />
                        </Grid.ColumnDefinitions>
                        <GroupBox Grid.Row="0" Grid.Column="0"
                  Margin="5,2,10,2" Foreground="White"
                  Style="{StaticResource GroupBoxStyle1}"
                  x:Name="gbUpLine" Grid.ColumnSpan="2" Visibility="Hidden">
                            <GroupBox.Header>
                                <Run FontWeight="Bold" FontSize="12">상행 제어</Run>
                            </GroupBox.Header>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1.0*" />
                                    <ColumnDefinition Width="1.0*" />
                                    <ColumnDefinition Width="1.0*" />
                                    <ColumnDefinition Width="1.0*" />
                                </Grid.ColumnDefinitions>

                                <StackPanel Orientation="Horizontal" Margin="152,-24,0,8" 
                            Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="4">
                                    <Label FontSize="16" VerticalAlignment="Center" Margin="10,0,5,0">단계</Label>
                                    <TextBox Text="{Binding H70A6}" VerticalAlignment="Center"
                               Foreground="Navy" FontWeight="Bold" IsReadOnly="True"
                               FontSize="16" Background="AliceBlue"
                               BorderThickness="2" Width="30" HorizontalContentAlignment="Right" />
                                    <Label FontSize="16" VerticalAlignment="Center" Margin="30,0,5,0" Content="내부휘도" Width="77"/>
                                    <TextBox Text="{Binding H70A870A9}" VerticalAlignment="Center"
                             Foreground="Navy" FontWeight="Bold" IsReadOnly="True"
                             FontSize="16" Background="AliceBlue" 
                             BorderThickness="2" Width="60" HorizontalContentAlignment="Right" />
                                    <Label FontSize="16" VerticalAlignment="Center" Margin="0,0,5,0">cd/㎡</Label>
                                    <Label FontSize="16" VerticalAlignment="Center" Margin="20,1,5,1" Content="외부휘도"/>
                                    <TextBox Text="{Binding H70AA70AB}" VerticalAlignment="Center"
                             Foreground="Navy" FontWeight="Bold" IsReadOnly="True"
                             FontSize="16" Background="AliceBlue" 
                             BorderThickness="2" Width="60" HorizontalContentAlignment="Right" Margin="0,4" />
                                    <Label FontSize="16" VerticalAlignment="Center" Margin="0,1,5,1" Content="cd/㎡" />
                                    <StackPanel Width="109" Margin="0,4,0,0">
                                        <ToggleButton x:Name="tbUpDimSet" Width="102" Margin="10,0,0,0" Style="{StaticResource PopupToggleButtonStyle}" RenderTransformOrigin="0,0.833">
                                            <StackPanel Orientation="Horizontal">
                                                <TextBlock Text="디밍 출력" Width="71" FontSize="16" VerticalAlignment="Center" HorizontalAlignment="Left"/>
